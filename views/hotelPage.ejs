                                <h5 class="mb-3"><i class="fa fa-random text-muted mr-2"></i><%= translation.accom_booking_info %></h5>
                                <!-- <div class="form-inline"> -->
                                    <input type="text" id="guest-name" class="form-control form-control-long" placeholder="Tên khách đại diện">
                                    <label class="ml-2 mr-2"></label>
                                    <input type="text" id="guest-contact" class="form-control form-control-long" placeholder="email / điện thoại">
                                <!-- </div> -->
                            </div>
                            
                            <div class="col-lg-3 form-group">
                                <h5 class="mb-3"><i class="fa fa-random text-muted mr-2"></i><%= translation.accom_room %></h5>
                                <!-- <input type="text" class="form-control form-control-long" placeholder="Loại phòng"> -->
                                
                                <div class="dropdown accom">
                                    <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="width:100%">
                                        Superior
                                    </button>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton" id="room-type">
                                      <a class="dropdown-item" href="#">Superior</a>
                                      <a class="dropdown-item" href="#">Deluxe</a>
                                      <a class="dropdown-item" href="#">Suite City View</a>
                                      <a class="dropdown-item" href="#">Suite Sea View</a>
