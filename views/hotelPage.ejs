                        <div class="card">
                            <img src="img/hotel/restaurant.jpg" alt="" class="card-image">
                            <div class="card-block">
                                <h5 class="mb-0"><%=  translation.utilities_buffet %></h5>
                                <p class="text-muted mb-0"><%=  translation.utilities_buffet_intro %></p>
                            </div>
                        </div>
                        <!-- Card -->
