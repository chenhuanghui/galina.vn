                                        <ul class="object-details list-unstyled">
                                            <li><span class="text-muted"><%= translation.square %>   </span> <%= roomlist[i].square %> m<sup>2</sup></li>
                                            <li><span class="text-muted"><%= translation.windows %>  </span> <%= roomlist[i].windows %></li>
                                            <li><span class="text-muted"><%= translation.balcone %>  </span> <%= roomlist[i].balcone %></li>
                                            <li><span class="text-muted"><%= translation.view %>     </span> <%= roomlist[i].view %></li>
                                            <li><span class="text-muted"><%= translation.capacity %> </span> 02 <%= translation.adult %>, 02 <%= translation.kid %></li>
                                        </ul>
                                        <span class="object-price"> <%= roomlist[i].price %> VND <sup><%= translation.night %></sup> </span>
                                        <a href="<%= roomlist[i].link %>" class="object-btn btn btn-secondary"><span class="hidden-xs-down"></span> <%= translation.detail %></a>
                                    </div> 
                                </div>
                            </div>
                            
                            <% } %>
                        </div>
                    </div>
                  </div>  
            </div>
  
        </section> 
  
        <!-- Section / Features -->
        <section id="features" class="section">
  
            <div class="container">
                <div class="row align-items-center" data-local-scroll>
                    <div class="col-lg-4">
