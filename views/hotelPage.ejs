                <div class="bg-image-holder bg-image-fixed zooming"><img src="img/photos/search-bg.jpg" alt=""></div>
                <div class="container">
                    <h1 class="mb-1"> <%= translation.accom_welcome1 %> </h1>
                    <p class="lead text-muted"> <%= translation.accom_welcome2 %> </p>
                    <hr>
                    <form action="#">
                        <div class="row align-items-end">
                            <div class="col-lg-3 col-md-4 form-group">
                                <h5 class="mb-3"><i class="fa fa-arrows-alt text-muted mr-2"></i> <%= translation.accom_in_out %> </h5>
                                <!-- <div class="form-inline"> -->
                                    <input type="text" id ="date-check-in" class="datepicker form-control form-control-long" placeholder="Select Date..">
                                    <label class="ml-2 mr-2"></label>
                                    <input type="text" id ="date-check-out" class="datepicker form-control form-control-long" placeholder="Select Date..">
                                <!-- </div> -->
                            </div>
                            
                            <div class="col-lg-3 col-md-4 form-group">
                                <h5 class="mb-3"><i class="fa fa-bed text-muted mr-2"></i><%= translation.accom_adult_kid %></h5>
                                <!-- <div class="form-inline"> -->
                                    <input type="text" id="adult" class="form-control form-control-long" placeholder="Nguời lớn">
                                    <label class="ml-2 mr-2"></label>
                                    <input type="text" id="kid" class="form-control form-control-long" placeholder="Trẻ em">
                                    <!-- <label class="ml-2">khách</label> -->
                                <!-- </div> -->
                            </div>
  
